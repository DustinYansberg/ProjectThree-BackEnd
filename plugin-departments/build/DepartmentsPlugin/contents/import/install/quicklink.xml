<sailpoint>

  <ImportAction name="merge">
    <Configuration name="SystemConfiguration">
      <Attributes>
        <Map>
          <entry key='quickLinkCategories'>
            <value>
              <List>
                <QuickLinkCategory name="EPlugin" messageKey="Manage Departments" enabled="true" icon="quickLinkCategoryCustom" />
              </List>
            </value>
          </entry>
        </Map>
      </Attributes>
    </Configuration>
  </ImportAction>

  <QuickLink action="external" category="EPlugin" messageKey="Manage Departments" name="Department Management" ordering="4">
    <Attributes>
      <Map>
        <entry key="url" value="plugins/pluginPage.jsf?pn=DepartmentsPlugin"/>
      </Map>
    </Attributes>
    <QuickLinkOptions allowSelf="true">
      <DynamicScopeRef>
        <Reference class="sailpoint.object.DynamicScope" name="Everyone"/>
      </DynamicScopeRef>
    </QuickLinkOptions>
  </QuickLink>

</sailpoint>